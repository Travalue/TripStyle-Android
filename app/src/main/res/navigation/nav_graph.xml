<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/registerFragment">

    <fragment
        android:id="@+id/trailerFragment"
        android:name="com.tripstyle.tripstyle.ui.trailer.TrailerFragment"
        android:label="TrailerFragment" >
        <action
            android:id="@+id/action_trailerFragment_to_categoryDialogFragment"
            app:destination="@id/categoryDialogFragment" />
        <argument
            android:name="argsString"
            app:argType="string"
            android:defaultValue=""/>
        <action
            android:id="@+id/action_trailerFragment_to_trailerDetailFragment"
            app:destination="@id/trailerDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/travellerFragment"
        android:name="com.tripstyle.tripstyle.ui.traveller.TravellerFragment"
        android:label="TravellerFragment" >
        <action
            android:id="@+id/action_travellerFragment_to_categoryDialogFragment"
            app:destination="@id/categoryDialogFragment" />
        <argument
            android:name="argsString"
            app:argType="string"
            android:defaultValue=""/>
        <action
            android:id="@+id/action_travellerFragment_to_TrailerSearchFragment"
            app:destination="@+id/trailerSearchFragment"/> <!-- 임시 -->
        <action
            android:id="@+id/action_travellerFragment_to_TravellerWriteFragment"
            app:destination="@+id/travellerWriteFragment"/>
    </fragment>

    <fragment
        android:id="@+id/trailerSearchFragment"
        android:name="com.tripstyle.tripstyle.ui.traveller.TrailerSearchFragment"
        android:label="TrailerSearchFragment">
    </fragment>

    <fragment
        android:id="@+id/travellerWriteFragment"
        android:name="com.tripstyle.tripstyle.ui.traveller.TravellerWriteFragment"
        android:label="TravellerWriteFragment">
        <action
            android:id="@+id/action_travellerWriteFragment_to_TravellerLocationFragment"
            app:destination="@+id/travellerLocationFragment"/>
    </fragment>

    <fragment
        android:id="@+id/travellerLocationFragment"
        android:name="com.tripstyle.tripstyle.ui.traveller.TravellerLocationFragment"
        android:label="TravellerLocationFragment">
    </fragment>

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.tripstyle.tripstyle.ui.info.info.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.tripstyle.tripstyle.ui.info.info.RegisterFragment"
        android:label="RegisterFragment" >
        <action
            android:id="@+id/action_registerFragment_to_trailerFragment"
            app:destination="@id/trailerFragment" />
        <action
            android:id="@+id/action_registerFragment_to_registerOkFragment"
            app:destination="@id/registerOkFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerOkFragment"
        android:name="com.tripstyle.tripstyle.ui.info.info.RegisterOkFragment"
        android:label="RegisterOkFragment" >
        <action
            android:id="@+id/action_registerOkFragment_to_trailerFragment"
            app:destination="@id/trailerFragment" />
        <action
            android:id="@+id/action_registerOkFragment_to_categoryDialogFragment"
            app:destination="@id/categoryDialogFragment" />
    </fragment>
    <dialog
        android:id="@+id/categoryDialogFragment"
        android:name="com.tripstyle.tripstyle.ui.info.info.CategoryDialogFragment"
        android:label="CategoryDialogFragment" >
        <action
            android:id="@+id/action_categoryDialogFragment_to_travellerFragment"
            app:destination="@id/travellerFragment" />
        <action
            android:id="@+id/action_categoryDialogFragment_to_trailerFragment"
            app:destination="@id/trailerFragment" />
        <argument
            android:name="argsString"
            app:argType="string"
            android:defaultValue=""/>
        <action
            android:id="@+id/action_categoryDialogFragment_to_myPageFragment"
            app:destination="@id/myPageFragment" />
    </dialog>
    <fragment
        android:id="@+id/trailerDetailFragment"
        android:name="com.tripstyle.tripstyle.ui.trailer.TrailerDetailFragment"
        android:label="trailer">

        <deepLink app:uri="https://guide.trailer.com/{postId}"
            app:action="android.intent.action.MY_ACTION"
            android:autoVerify="true"/>
        <argument
            android:name="postId"
            app:argType="integer"
            android:defaultValue="1" />
    </fragment>
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.tripstyle.tripstyle.ui.mypage.EditProfileFragment"
        android:label="EditProfileFragment" >
        <action
            android:id="@+id/action_editProfileFragment_to_myPageFragment"
            app:destination="@id/myPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/myPageFragment"
        android:name="com.tripstyle.tripstyle.ui.mypage.MyPageFragment"
        android:label="MyPageFragment" >
        <action
            android:id="@+id/action_myPageFragment_to_categoryDialogFragment"
            app:destination="@id/categoryDialogFragment" />
        <argument
            android:name="argsString"
            app:argType="string"
            android:defaultValue=""/>
        <action
            android:id="@+id/action_myPageFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
        <action
            android:id="@+id/action_myPageFragment_to_shareTravelFragment"
            app:destination="@id/shareTravelFragment" />
        <action
            android:id="@+id/action_myPageFragment_to_myTravelListFragment"
            app:destination="@id/myTravelListFragment" />
        <action
            android:id="@+id/action_myPageFragment_to_likeListFragment"
            app:destination="@id/likeListFragment" />
    </fragment>
    <fragment
        android:id="@+id/myTravelListFragment"
        android:name="com.tripstyle.tripstyle.ui.mypage.MyTravelListFragment"
        android:label="MyTravelListFragment" >
        <action
            android:id="@+id/action_myTravelListFragment_to_myPageFragment"
            app:destination="@id/myPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/likeListFragment"
        android:name="com.tripstyle.tripstyle.ui.mypage.LikeListFragment"
        android:label="LikeListFragment" >
        <action
            android:id="@+id/action_likeListFragment_to_myPageFragment"
            app:destination="@id/myPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/shareTravelFragment"
        android:name="com.tripstyle.tripstyle.ui.mypage.ShareTravelFragment"
        android:label="공유중인 여행지"
        tools:layout="@layout/fragment_share_travel" >
        <action
            android:id="@+id/action_shareTravelFragment_to_shareTravelDetailFragment"
            app:destination="@id/shareTravelDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/shareTravelDetailFragment"
        android:name="com.tripstyle.tripstyle.ui.mypage.ShareTravelDetailFragment"
        android:label="{type}" >
        <argument
            android:name="type"
            app:argType="string" />
        <action
            android:id="@+id/action_shareTravelDetailFragment_to_categoryEditFragment"
            app:destination="@id/categoryEditFragment" />
    </fragment>
    <fragment
        android:id="@+id/categoryEditFragment"
        android:name="com.tripstyle.tripstyle.ui.mypage.CategoryEditFragment"
        android:label="카테고리 수정" />

</navigation>